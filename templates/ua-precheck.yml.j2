defaultHostCredentials:
  sshUserName: {{ username }}
  sshPassword: {{ password_b64 }}
  sshPort: 22
sudoPrefix: ""
prechecks:
  coordinator:
    controlplane:
      - host: {{ orchestrator }}
  ua:
    controlplane:
      - host: {{ controlplane }}
    worker:
      {% for worker in workers %}
- host: {{ worker }}
      {% endfor %}
